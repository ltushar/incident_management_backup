<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog"
		title="Employee's Supervsior"
		items="{/EmpInfo}"
		search=".onValueHelpSearch"
		confirm="onPressValuHelp"
		cancel=".onValueHelpClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{supname}" 
            description="{supid}"/>
	</SelectDialog>
</core:FragmentDefinition>